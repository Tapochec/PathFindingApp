<UserControl x:Class="PathFindingApp.View.Settings.Pages.GlobalSettings"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PathFindingApp.View.Settings.Pages"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <CheckBox x:Name="EightWayCheckBox" Content="8 way" Margin="10 10 0 0" VerticalAlignment="Top" HorizontalAlignment="Left" Unchecked="EightWay_CheckBox_Checked" Checked="EightWay_CheckBox_Checked"/>
    </Grid>
</UserControl>
